    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Codificar\\Sms\\SmsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev" 
}